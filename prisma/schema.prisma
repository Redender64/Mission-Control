// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./server.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id              String  @unique
  money           Float   @default(10000)
  eth             Float   @default(0)
  leastearnstamp  Int     @default(0)
}