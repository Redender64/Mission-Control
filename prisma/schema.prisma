// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgres"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id             String   @id
  money          Float    @default(10000)
  eth            Float    @default(0)
  leastearnstamp DateTime @default(now())
}
